{
    "globals": {
        "ajaxObject": true,
        "require": true
    },    
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console":0,
        "space-in-parens": ["error", "never"],
        "no-trailing-spaces": 2,
        "camelcase": [2,{"properties":"always"}],
        "space-before-blocks": [2,"always"],
        "eol-last": 2,
        "no-multiple-empty-lines": [2,{"max":1}]
    }
}