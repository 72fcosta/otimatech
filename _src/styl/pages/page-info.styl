.page.info
    .page-content
        .sidemain
            .part-content
                .part-content-inner
                    margin-bottom 20px
                    .medidas
                        lost-utility clearfix
                        .medidas-title
                            lost-column 1/1 4 0
                            background: colors
                            color white
                            padding: 5px 5px 5px 10px
                            font-weight: bold
                            margin-top: 25px
                        .medidas-aviso
                            font-style: italic
                            lost-column 1/1 4 0
                        .medidas-img, .medidas-table
                            border 4px solid colors
                            background: white
                            padding: 20px
                            height 217px
                            margin-top: 5px
                        .medidas-img
                            lost-column 1/4 4 0
                            text-align: center
                            padding-top: 9px
                            img
                                width auto
                                height 100%
                        .medidas-table
                            lost-column 3/4 4
                            .medidas-color-1
                                background: #FF0000
                            .medidas-color-2
                                background: #6666CC
                            .medidas-color-3
                                background: #FFFF00
